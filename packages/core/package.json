{
  "name": "@parsrun/core",
  "version": "0.1.7",
  "description": "Core utilities and types for Pars framework - Edge compatible",
  "keywords": ["pars", "core", "utilities", "edge", "cloudflare", "deno", "bun"],
  "homepage": "https://pars.run",
  "repository": {
    "type": "git",
    "url": "https://github.com/parsrun/pars",
    "directory": "packages/core"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "import": "./dist/env.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js"
    },
    "./decimal": {
      "types": "./dist/decimal.d.ts",
      "import": "./dist/decimal.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./transports": {
      "types": "./dist/transports/index.d.ts",
      "import": "./dist/transports/index.js"
    },
    "./error-codes": {
      "types": "./dist/error-codes.d.ts",
      "import": "./dist/error-codes.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
