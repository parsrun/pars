{
  "name": "@parsrun/storage",
  "version": "0.2.10",
  "description": "Edge-compatible storage abstraction for Pars - S3, R2, DO Spaces",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapters/s3": {
      "types": "./dist/adapters/s3.d.ts",
      "import": "./dist/adapters/s3.js"
    },
    "./adapters/r2": {
      "types": "./dist/adapters/r2.d.ts",
      "import": "./dist/adapters/r2.js"
    },
    "./adapters/memory": {
      "types": "./dist/adapters/memory.d.ts",
      "import": "./dist/adapters/memory.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "storage",
    "s3",
    "r2",
    "cloudflare",
    "edge",
    "pars"
  ],
  "author": "EMS Tech Solutions",
  "license": "MIT",
  "homepage": "https://pars.run",
  "repository": {
    "type": "git",
    "url": "https://github.com/parsrun/pars",
    "directory": "packages/storage"
  },
  "dependencies": {
    "@parsrun/core": "workspace:*",
    "@parsrun/types": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@aws-sdk/s3-request-presigner": {
      "optional": true
    }
  }
}
